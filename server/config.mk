# Name of the generated executable binary.
NAME    = server

# Directory containing all C sources
SRC_DIR = src

# Directory containing all C headers
INC_DIR = include

# Directory containing all generated objects.
OUT_DIR = _build

# The compiler to use.
CC      = gcc

# The linker to use.
LD      = gcc

# The list of dependencies to use.
CDEPS   = 

# Custom flags given to the compiler.
CFLAGS  = -O2 -Wall

# Custom flags given to the linker.
LDFLAGS =

